{
    "version": "1.0.7",
    "push": [
        "github"
    ],
    "build": {
        "standalone": {
            "wrap": true,
            "global": true,
            "target": "dist/metaphorjs.watchable.js",
            "files": [
                [
                    "src/metaphorjs.watchable.js",
                    {
                        "as": "Watchable"
                    }
                ]
            ],
            "expose": [
                [
                    "MetaphorJs.lib",
                    "Watchable"
                ]
            ]
        },
        "npm": {
            "target": "dist/metaphorjs.watchable.npm.js",
            "compile": false,
            "files": [
                [
                    "src/metaphorjs.watchable.js",
                    {
                        "keepExports": true
                    }
                ]
            ],
            "omit": [
                "../metaphorjs-observable/src/metaphorjs.observable.js"
            ],
            "require": {
                "metaphorjs-observable": "Observable"
            }
        },
        "amd": {
            "auto": false,
            "target": "dist/metaphorjs.watchable.amd.js",
            "files": [
                [
                    "src/metaphorjs.watchable.js",
                    {
                        "returnExports": true
                    }
                ]
            ],
            "omit": [
                "../metaphorjs-observable/src/metaphorjs.observable.js"
            ],
            "define": {
                "name": "metaphorjs-watchable",
                "deps": {
                    "metaphorjs-observable": "Observable"
                }
            }
        }
    }
}