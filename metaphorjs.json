{

    "standalone": {
        "wrap": true,
        "global": true,
        "target": "dist/metaphorjs.watchable.js",
        "files": [
            ["src/metaphorjs.watchable.js", {"as": "Watchable"}]
        ],
        "expose": [
            ["MetaphorJs.lib", "Watchable"]
        ]
    },

    "npm": {
        "target": "dist/metaphorjs.watchable.npm.js",
        "files": [
            ["src/metaphorjs.watchable.js", {"keepExports": true}]
        ],
        "omit": [
            "../metaphorjs-observable/src/metaphorjs.observable.js"
        ],
        "require": {
            "metaphorjs-observable": "Observable"
        }
    },

    "amd": {
        "target": "dist/metaphorjs.watchable.amd.js",
        "files": [
            ["src/metaphorjs.watchable.js", {"returnExports": true}]
        ],
        "omit": [
            "../metaphorjs-observable/src/metaphorjs.observable.js"
        ],
        "define": {
            "name": "metaphorjs-watchable",
            "deps": {
                "metaphorjs-observable": "Observable"
            }
        }
    },

    "compile-standalone": {
        "target": "dist/metaphorjs.watchable.min.js",
        "compile": "standalone"
    },

    "compile-amd": {
        "target": "dist/metaphorjs.watchable.amd.min.js",
        "compile": "amd"
    }
}